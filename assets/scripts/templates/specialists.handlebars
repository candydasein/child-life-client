{{#each specialists as |specialist|}}
  {{log specialist}}

  <div class="container">
    <section data-id="{{specialist.id}}" class="specialists">
   
    {{!-- <h5 id ="patient-screen_name"><small></small></h5>  --}}
    <div class="row">
         <div class="col-sm" style="font-size:40px;vertical-align:-50%">{{specialist.first_name}}</div>
         {{!-- <div class="col-sm">{{specialist.last_name}}</div> --}}
         <br>
         <div class="col-sm"><img style="object-fit:cover;height:200px;border-radius:40px" src="{{specialist.picture}}"></div>
    <div class="col-sm"><button style="margin-top:6px;vertical-align:-50%" type="submit" id="contact-specialist">Contact</button></div>
    </div>
    <br>
    
    </section>
  </div>
{{/each}}